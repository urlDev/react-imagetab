{
    "manifest_version": 2,
    "name": "ImageTab",
    "author": "Can Ural",
    "version": "1.0.1",
    "description": "Replace new tab screen with beautiful images",
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": ["storage"]
}